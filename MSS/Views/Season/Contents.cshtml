@model MSS.Models.DateModel
@{
    ViewBag.Title = Model.Title + " TV Schedule By Week";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<nav class="navbar DONTPrint">
	<div class="container">
		<div>
			@Html.Partial("AddThis/AddThis")
			@Html.Partial("Google/GoogleSearchBar")
			@Html.Partial("AddThis/AddThisVar")
		</div>
		<div style="clear:both; padding-bottom: 5px"></div>
		<div>
			<a href="@Url.Content("~/")">Home</a><br />
		</div>
	</div>
</nav>
<div id="Main">
	<p>@Html.Label(Model.Title)</p>
	<div id="content">
		<div id="SeasonLinks" class="DONTPrint">
			@Html.Partial("YearDates")
		</div>
		@if (Model.IsFootball)
		{
			@Html.Partial("ConferenceList/" + Model.ConferenceListBase)
		}
	</div>
	<div style="display: inline-block">
		<p>
			<span id="Label9">
				Got a question, complaint, comment or know a game not listed here?
			</span><a id="HyperLink32" href="mailto:footballsked@gmail.com">Send it here</a>
		</p>
	</div>
</div>
@section scripts{
    <script defer="defer" type="text/javascript">
		$(document).ready(function () {
			$("#content").css("min-height", $("#SeasonLinks").height());
			$("#Conference").css("min-height", $("#SeasonLinks").height());
		});
</script>

}
@section styles {
	<style type="text/css">#SeasonLinks,#Conference{float:left;display:inline}#SeasonLinks{min-height:250px;}#Conference{width:35%;min-height:380px;}#Main{padding-top:42px}.navbar {box-shadow: 0 1px 1px rgba(0,0,0,.1);}</style>
    <style media="all and (min-width: 641px)" type="text/css">#SeasonLinks{width:55%;}</style>
    <style type="text/css" media="only screen and (max-width: 640px)">#SeasonLinks{width: 65%;}.DONTPrint a{line-height:13px;}</style>
}

