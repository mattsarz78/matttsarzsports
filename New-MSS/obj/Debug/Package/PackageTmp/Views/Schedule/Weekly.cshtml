@model New_MSS.Models.WeeklyModel
@{
    ViewBag.Title = "Week " + Model.Week;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<form action="@ViewBag.ActionName" id="WeekForm" method="post">
<p>
    @RenderSection("YearLink", required: true)
    @RenderSection("BlogLink", required: false)
    @RenderSection("FlexWindows", required: false)
    @RenderSection("TextLink", required: false)
</p>
<div>
    @if (Model.ShowRSNPartialView)
    {
        @Html.Partial("CoverageNotes/FSNWeek" + Model.Week)
    }
    @Html.Partial("Weekly/WeeksBase")
    @if (Model.ShowNoTVPartialView) 
    {
        @Html.Partial("Weekly/NoTV", Model)
    }
</div>
</form>
